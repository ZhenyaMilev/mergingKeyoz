{% assign creditCardImg = 'credit-card.jpeg' | asset_url %}
{% assign in_rings_collection = false %}
{% for collection in product.collections %}
  {% if collection.handle == 'rings' %}
    {% assign in_rings_collection = true %}
  {% endif %}
{% endfor %}

