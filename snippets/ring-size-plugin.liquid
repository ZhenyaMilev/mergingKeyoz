          d="M16.177 1.208a.709.709 0 011.002 0l5.613 5.613a.709.709 0 010 1.002L7.823 22.793a.709.709 0 01-1.002 0l-5.613-5.614a.709.709 0 010-1.002zm.5 1.503l-2.272 2.272 1.37 1.37a.709.709 0 01-.922 1.071l-.08-.069-1.37-1.37-1.804 1.804 1.37 1.37a.709.709 0 01-.923 1.072l-.08-.069-1.37-1.37-1.804 1.804 1.37 1.37a.709.709 0 01-.923 1.071l-.08-.068-1.369-1.371-1.805 1.804 1.37 1.37a.709.709 0 01-.922 1.072l-.08-.069-1.37-1.37-2.272 2.273 4.61 4.61L21.289 7.323l-4.61-4.61z"
        ></path>
      </svg>
      <button class="ring-adjuster__main-btn__show">{{ 'ring_size_plugin.button_text' | t }}</button>
      <div class="ring-adjuster__card-detect">
        <div class="ring-callibration__container">
          <div class="calibration-card__info-container">
            <div class="calibration-card__info-nav">
              <span class="calibration-card__info-nav--arrow">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 32 32">
                  <path
                    d="M32 15H3.41l8.29-8.29-1.41-1.42-10 10a1 1 0 0 0 0 1.41l10 10 1.41-1.41L3.41 17H32z"
                    data-name="4-Arrow Left"
                  ></path>
                </svg>
              </span>
              <p>{{ 'ring_size_plugin.back_button_text' | t }}</p>
            </div>
            <div class="calibration-card__info">
              <p class="calibration-card__info-step">{{ 'ring_size_plugin.step1_title' | t }}</p>
              <p>
                {{ 'ring_size_plugin.step1_text' | t }}
              </p>
              <p class="calibration-card__info-specs">{{ 'ring_size_plugin.step1_bold_text' | t }}</p>
            </div>
          </div>
          <div class="calibration-card__ring-container">
            <div
              id="calibration-card__ring"
              class="calibration-card__ring-img"
              style="background-image: url('{{ creditCardImg }}')"
            ></div>
          </div>
          <div class="calibration-card__range-button">
            <div class="calibration-card__range-container">
              <input type="range" id="ring-adjuster__range-cs" class="calibration-card__range" min="0" max="100" />
            </div>
            <div class="calibration-card__button-container">
              <button class="calibration-card__btn">
                {{ 'ring_size_plugin.next_button_text' | t }}
              </button>
            </div>
          </div>
        </div>
      </div>
